{
  "db_name": "PostgreSQL",
  "query": "SELECT \n            t.id,\n            t.creator_user_id,\n            t.creator_name,\n            t.name,\n            t.slug,\n            t.short_description,\n            t.long_description,\n            c.name AS \"category_code?\",\n            t.product_id,\n            t.tags,\n            t.tech_stack,\n            t.status,\n            t.is_configurable,\n            t.view_count,\n            t.deploy_count,\n            t.required_plan_name,\n            t.created_at,\n            t.updated_at,\n            t.approved_at\n        FROM stack_template t\n        LEFT JOIN stack_category c ON t.category_id = c.id\n        WHERE t.status IN ('submitted', 'approved')\n        ORDER BY \n            CASE t.status\n                WHEN 'submitted' THEN 0\n                WHEN 'approved' THEN 1\n            END,\n            t.created_at ASC",

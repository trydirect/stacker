{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            s.id,\n            s.user_id,\n            s.project_id,\n            s.cloud_id,\n            c.provider as cloud,\n            s.region,\n            s.zone,\n            s.server,\n            s.os,\n            s.disk_type,\n            s.created_at,\n            s.updated_at,\n            s.srv_ip,\n            s.ssh_port,\n            s.ssh_user,\n            s.vault_key_path,\n            s.connection_mode,\n            s.key_status,\n            s.name\n        FROM server s\n        LEFT JOIN cloud c ON s.cloud_id = c.id\n        WHERE s.user_id=$1\n        ORDER BY s.created_at DESC\n        ",
